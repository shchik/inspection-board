{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"baseUrl": "./"
	},
	"include": ["src/**/*", "express.d.ts"],
	"exclude": ["node_modules"]
}
